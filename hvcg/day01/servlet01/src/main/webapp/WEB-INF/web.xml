<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
    <servlet-name>HelloServlet</servlet-name>
    <servlet-class>vn.hvcg.HelloServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>HelloServlet</servlet-name>
    <url-pattern>/hi</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>GoodbyeServlet</servlet-name>
    <servlet-class>vn.hvcg.GoodbyeServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>GoodbyeServlet</servlet-name>
    <url-pattern>/bye</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>HomeWork1</servlet-name>
    <servlet-class>vn.hvcg.HomeWork1</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>HomeWork1</servlet-name>
    <url-pattern>/hello</url-pattern>
  </servlet-mapping>
</web-app>
